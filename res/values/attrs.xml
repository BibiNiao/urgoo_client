<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ContactItemView">
        <attr name="contactItemImage" format="reference" />
        <attr name="contactItemName" format="string" />
    </declare-styleable>

    <declare-styleable name="LCircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="im_border_width" format="dimension" />
        <attr name="im_border_color" format="color" />
    </declare-styleable>
    <declare-styleable name="FlashView">
        <attr name="effect">
            <enum name="accordion" value="0"/>
            <enum name="cube" value="1"/>
            <enum name="def" value="2"/>
            <enum name="depthPage" value="3"/>
            <enum name="inRightDown" value="4"/>
            <enum name="inRightUp" value="5"/>
            <enum name="rotate" value="6"/>
            <enum name="zoomOutPage" value="7"/>

        </attr>
    </declare-styleable>

    <declare-styleable name="LoadingView">

        <attr name="loadingText" format="string"/>
        <attr name="loadingTextAppearance" format="reference"/>
    </declare-styleable>

    <declare-styleable name="XlHRatingBar">
        <attr name="starCount" format="integer" /><!--总的星星数-->
        <attr name="canEdit" format="boolean" /><!--是否可以点击选择，有时候纯展示用不需要选择-->
        <attr name="widthAndHeight" format="dimension" /><!--单个星星的宽度-->
        <attr name="dividerWidth" format="dimension" /><!--星星左右间距-->
        <attr name="countSelected" format="integer" /><!--当前已选中的星星个数-->
        <attr name="stateResId" format="reference" /><!--单个星星不同状态下对应的图标-->
        <attr name="differentSize" format="boolean" /><!--是否支持大小不等星星样式-->
    </declare-styleable>


</resources>
